  <!-- Global Settings -->
  <style name="selection"         background="#8dac6f"/> <!-- #8dac6f --><!-- mint green --> <!-- #708b99 -->
  <style name="text"              foreground="#f8f8f8" background="#2b2b2b"/> <!-- #2d2d2d  #2b2b2b -->
  <style name="cursor"            foreground="#ffffff"/>
  <style name="current-line"      background="#393b3d"/> <!-- original #FF0000 - #132733 -->
  <style name="line-numbers"      foreground="#c4c2c2" background="#393b3d"/> <!-- original foreground: #f8f8f8 opt1: #bbbbbb - background: #393b3d opt1: #313335 - #1b3849 -->

  <!-- Bracket Matching -->
  <style name="bracket-match" foreground="#ffef2e" background="#4C4C4C"/> <!-- bright blue #00b6ff mint green #8dac6f -->
  <style name="bracket-mismatch" background="#4C4C4C" bold="true"/>
  <style name="search-match" background="#f8f300" foreground="#red"/> <!-- original: #404040 -->
  <!-- Monokai-extended: <style name="search-match"  background="#333333" bold="true" underline="true"/> -->
  <style name="match-style" background="#f8f300" foreground="#red"/> <!-- original: #404040 -->
  <style name="def:comment" foreground="#8c876f"/>
  

  <!-- My old styles
  <style name="def:comment" foreground="#f8f8f8"/>
  <style name="def:doc-comment-element" foreground="#f8f8f8" italic="true"/>
  <style name="def:shebang" foreground="#f8f8f8">
  <style name="def:constant" foreground="#66d9ef" italic="true"/>
  <style name="def:special-char" foreground="#f8f8f8"/>
  <style name="def:identifier" foreground="#e9256c"/>
  <style name="def:statement" foreground="#a6e12e" bold="true"/>
  <style name="def:type" foreground="#orange" italic="true"/>
  <style name="def:error" background="#C80000" foreground="#F0EA20"/> 
  <style name="def:note" background="#F0EA20" foreground="#C80000"/>
  -->
  
  <!-- Annotations about old styles: 
  - name="def:constant": image title
  - name="def:identifier": code span - v1 d63384 v2 e9256c
  - name="def:statement": list bullets
  - name="def:type": titles c455ff - f700ff - d01ad7
  -->
  
  <!-- Custom Markdown Codebits Styling -->
  <style name="markdown-extra-tech:ipsum" foreground="#orange"/> <!-- FOREGROUND original: #f8f8f8 -->
  <style name="markdown-extra-tech:language-code-block" italic="true" foreground="#d01ad7" />
  <style name="markdown-extra-tech:header" foreground="#00dd2d"/>
  <style name="markdown-extra-tech:header-id-marker" foreground="#ff0083"/>
  <style name="markdown-extra-tech:attribute-value" foreground="#ff0083"/>
  <style name="markdown-extra-tech:attribute-braces" foreground="#00b2ec"/>
  <style name="markdown-extra-tech:link-brackets" foreground="#00b2ec"/>
  <style name="markdown-extra-tech:link-text" foreground="#00b2ec"/>
  <style name="markdown-extra-tech:link-url" foreground="#f8f8f8"/>
  <style name="markdown-extra-tech:link-label" foreground="#fffb00"/> <!-- aqua #00ffb7 - light-yellow #fffb00 -->
  <style name="markdown-extra-tech:link-automatic" foreground="#00b2ec"/>
  <style name="markdown-extra-tech:image-marker" foreground="#ff5400"/>
  <style name="markdown-extra-tech:image-brackets" foreground="#ff5400"/>
  <style name="markdown-extra-tech:custom-attributes-braces" foreground="#00b2ec"/>
